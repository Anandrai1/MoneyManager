<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fintrackindia.moneymanager">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />


    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".spends.SpendsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".spends.AddExpenseActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".spends.categories.CategoriesActivity" />
        <activity android:name=".spends.categories.AddCategoryActivity" />
        <activity android:name=".CalculatorActivity" />
        <activity android:name=".investments.InvestmentsActivity" />
        <activity android:name=".investments.AddInvestmentActivity" />
        <activity android:name=".insurance.InsurancesActivity" />
        <activity android:name=".calculator.InvCalculatorActivity" />
        <activity android:name=".readsms.ViewAllMassageActivity" />
        <activity android:name=".onlie_invest.OnlieInvestActivity" />
        <activity android:name=".planning.PlanningActivity" />
        <activity android:name=".spends.ExpenseDetailActivity"></activity>
    </application>

</manifest>